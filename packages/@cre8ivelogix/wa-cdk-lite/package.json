{
  "name": "@cre8ivelogix/wa-cdk-lite",
  "version": "2.66.1",
  "description": "Well Architected CDK Constructs - Lite",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --verbose",
    "jsii-build": "jsii -vvv",
    "package": "jsii-pacmak -vvv",
    "build-test": "npm install && npm run build && npm run test",
    "doc-gen": "npx typedoc",
    "pretty": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cre8ivelogix-inc/wa-cdk-lite.git",
    "directory": "packages/@cre8ivelogix/wa-cdk-lite"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/cre8ivelogix-inc",
  "keywords": [
    "aws",
    "cdk",
    "well architected",
    "constructs",
    "cloud development kit",
    "xler8r"
  ],
  "contributors": [
    {
      "name": "Muhammad S Tahir",
      "email": "muhammad.tahir@cre8ivelogix.com"
    }
  ],
  "author": {
    "name": "Muhammad S Tahir <muhammad.tahir@cre8ivelogix.com>"
  },
  "license": "MIT-0",
  "jsii": {
    "outdir": "dist",
    "excludeTypescript": ["**/*.test.ts"],
    "versionFormat": "short",
    "targets": {
      "python": {
        "distName": "wa-cdk-lite",
        "module": "wa-cdk-lite"
      },
      "java": {
        "package": "com.cre8ivelogix",
        "maven": {
          "groupId": "com.cre8ivelogix",
          "artifactId": "wa-cdk-lite"
        }
      },
      "dotnet": {
        "namespace": "WaCDKLite",
        "packageId": "WaCDKLite"
      },
      "go": {
        "moduleName": "github.com/cre8ivelogix-inc",
        "packageName": "wacdklite"
      }
    }
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.66.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.66.0",
    "@types/babel__traverse": "7.18.2",
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "aws-cdk": "2.66.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cdk": "^1.4.0",
    "eslint-plugin-jest": "^25.2.4",
    "jest": "^27.5.1",
    "prettier": "2.8.4",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.4.4",
    "jsii": "^1.75.0",
    "jsii-pacmak": "^1.75.0",
    "publib": "^0.2.601",
    "aws-cdk-lib": "2.66.0",
    "constructs": "10.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.66.0",
    "constructs": "^10.0.0"
  }
}
